{
  "name": "json-string-values-parser",
  "version": "0.1.1",
  "description": "Recursively traverse JSON properties and call transformation function on the value",
  "keywords": [
    "json",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alxevvv/json-string-values-parser.git"
  },
  "license": "MIT",
  "author": "alxevvv <alxevvv@gmail.com>",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "cz": "git-cz",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint ./src/**/*.ts ./src/*.ts",
    "lint:fix": "eslint ./src/**/*.ts ./src/*.ts --fix",
    "prepare": "husky install",
    "test": "vitest run",
    "test:cov": "vitest run --coverage --silent",
    "test:watch": "vitest --watch"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitest/coverage-c8": "^0.31.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "tsup": "^6.7.0",
    "type-fest": "^3.11.1",
    "typescript": "^5.1.3",
    "vitest": "^0.31.4"
  }
}
